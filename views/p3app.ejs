<!DOCTYPE html>
<html lang="en">
	<head>
		<% include header %>
	</head>
	<body onunload="" class="claro patric p4-ui <% if (request.isAuthenticated && request.isAuthenticated()) { %>Authenticated<% } %>">

			<!-- New Navbar, CSS scoped to .patric-bootstrap -->
			<div class="patric_bootstrap">
				<% include devp3header %>
			</div>

			<div id="ApplicationContainer" data-dojo-type="dijit/layout/BorderContainer" class="layoutContainer" data-dojo-props="id:'ApplicationContainer',gutters:false,liveSplitters:false" style="height:calc(100% - 38px)">

			<div data-dojo-type="dijit/layout/ContentPane" data-dojo-props="region:'center'" style="padding:0px;border:0px solid #ddd;background:inherit;">

			</div>
			<div data-dojo-type="p3/widget/WorkspaceController" data-dojo-props="region:'bottom'"></div>
		</div>
		<% include javascript %>
	</body>
</html>
